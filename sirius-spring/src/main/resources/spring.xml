<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:sirius="http://sirius.org/schema/sirius"

	xsi:schemaLocation="http://www.springframework.org/schema/beans 
                        http://www.springframework.org/schema/beans/spring-beans.xsd
                        http://sirius.org/schema/sirius 
                        http://sirius.org/schema/sirius/sirius.xsd ">



   
	<sirius:reference id="apple"   interface="org.sirius.spring.Apple" directUrl="127.0.1" >
	   <sirius:method name = "get" timeout = "6077000">
	   <sirius:parameter key = "一个" value= "粒子"/>
	   <sirius:parameter key = "5个" value= "粒d子"/>
	 
	     <sirius:argument index ="4"></sirius:argument>
	   </sirius:method>
	   <sirius:method name = "eat" timeout = "30000">
	   <sirius:parameter key = "第三方5个" value= "水电费粒d子"/>
	     <sirius:argument index ="7"></sirius:argument>
	   </sirius:method>
	</sirius:reference>
	

</beans>